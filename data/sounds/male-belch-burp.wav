<script type="text/javascript"> 
    <!-- For version detection, set to min. required Flash Player version, or 0 (or 0.0.0), for no version detection. --> 
    var swfVersionStr = "10.0.0";
    <!-- To use express install, set to playerProductInstall.swf, otherwise the empty string. -->
    var xiSwfUrlStr = "";
    var flashvars = {};
    var params = {};
    params.quality = "high";
    params.bgcolor = "#869ca7";
    params.allowscriptaccess = "sameDomain";
    params.wmode = "transparent";
    var attributes = {};
    attributes.id = "play-sound-control";
    attributes.name = "play-sound-control";
    attributes.style = "position:absolute;top:10px;left:600px;";
    attributes.align = "middle";
    swfobject.embedSWF(                             
        "/swf/APlaySoundControll.swf", "play-sound-control", 
        //"340", "14", 
		'1','1',
        swfVersionStr, xiSwfUrlStr, 
        flashvars, params, attributes);
    
    var playVolumeControl = null;
    
    function initPlaySoundControl(){
        playVolumeControl = document.getElementById("play-sound-control");
        try{
          playVolumeControl.addControllsListener('receivePlaySoundControlEvents');
        }catch(e){
        }
    }

    Event.observe(window, 'load', function(){
    	initPlaySoundControl();
        //swfControlls.sendMsg('VOLUME',0.5);
        //swfControlls.sendMsg('SEEK',0.7);        
    });
    
    function receivePlaySoundControlEvents(msg,value) {
        switch(msg){
            case 'VOLUME':
                getRingtonePlayer().setVolume(value*100);
                break;
            case 'END_SEEK':
                getRingtonePlayer().setSeek(value*100);
                getRingtonePlayer().PlayPause();
                break;
            case 'SEEK':
                break;
            case 'MUTE':
            	getRingtonePlayer().mute();
            	break;
            case 'UNMUTE':
            	getRingtonePlayer().unmute();
            	break;	    
            case 'START_SEEK':
                getRingtonePlayer().PlayPause();
                break;
            default:
                alert(msg);
        }
    }
    
    function getPlayVolumeControl(){
        if(playVolumeControl == null){
        	initPlaySoundControl();
        }
        return playVolumeControl;
    }
    
</script>	
 

